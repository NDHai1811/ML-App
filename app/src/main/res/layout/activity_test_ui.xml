    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:text="Không có dữ liệu nào"
        android:visibility="invisible"/>


</RelativeLayout>